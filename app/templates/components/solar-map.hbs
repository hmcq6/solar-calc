{{#g-map
  lat="42.3600825"
  lng="-71.05888010000001"
  zoom=18
  onLoad=(action "onLoad")
  onClick=(action "onClick")
  as |g|
}}
  {{yield}}
  {{search-bar g=g map=map}}
  {{control-panel
    g=g
    map=map
    isDrawing=isDrawing
    points=points
    _polylines=_polylines
    _polygon=_polygon
    clearPoints="clearPoints"
    _updateCursor="_updateCursor"
    toggleIsDrawing="toggleIsDrawing"}}
{{/g-map}}
